    "karma-webpack": "^1.8.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.34.13",
    "rxjs": "^5.0.0-rc.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
